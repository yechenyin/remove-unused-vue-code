{
  "name": "remove-unused-vue-code",
  "displayName": "Remove Unused Vue Code",
  "description": "remove unused vue code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:remove-unused-vue-code.remove-unused-code"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "remove-unused-vue-code.remove-unused-code",
        "title": "Remove Unused Vue Code"
      }
    ],
    "keybindings": [
      {
        "command": "remove-unused-vue-code.remove-unused-code",
        "key": "alt+r",
        "mac": "alt+r",
        "when": "resourceLangId == vue"
      }
    ],
    "configuration": {
        "title": "Remove Unused Vue Code",
        "properties": {
            "remove-unused-vue-code.ignoreStylePrefix": {
                "type": "string",
                "default": "-",
                "description": "ignore style prefix name like 'el-', split by space"
            }
        }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@babel/generator": "^7.16.5",
    "@babel/parser": "^7.16.6",
    "@babel/traverse": "^7.16.5",
    "@babel/types": "^7.16.0",
    "vue-template-compiler": "^2.7.10"
  },
  "repository": "https://github.com/yechenyin/remove-unused-vue-code.git",
  "publisher": "yechenyin"
}
